{
  "editor.rulers": [
    80,
  ],
 "editor.tabSize": 2,
 "editor.insertSpaces": true,
 "files.eol": "\n",
 "editor.formatOnSave": true,
 "editor.formatOnType": true,
 "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
}
